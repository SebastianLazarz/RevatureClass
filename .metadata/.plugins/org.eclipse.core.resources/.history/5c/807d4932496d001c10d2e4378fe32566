package com.revature;

public class Q1 {
	
	public void bubbleSort(int[] array) {
		
		boolean done = false;
		int temp = 0;
		
		while (done == false) {
			done = true;
			
			for (int i= 0; i < array.length-1; i++) {
				if (array[i] > array[i+1]) {
					temp = array[i];
					array[i] = array[i+1];
					array[i+1] = temp;
					done = false;
				}
			}
		}
		
		
		
	}
}
